{
  "name": "react-turn",
  "version": "0.0.1",
  "description": "React UI Component base on turn.js",
  "main": "dist/cjs/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    "import": "./dist/esm/index.mjs",
    "require": "./dist/cjs/index.js",
    "types": "./dist/types/index.d.ts"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/bijinfeng/react-turn.git"
  },
  "license": "MIT",
  "scripts": {
    "clean": "shx rm -rf dist/",
    "build_types": "tsc -b .",
    "build_esm": "swc -C module.type=es6 -d dist/esm src/",
    "build_cjs": "swc -C module.type=commonjs -d dist/cjs src/",
    "esm_hack": "node ../../scripts/esmify.mjs",
    "build": "run-s build_types build_esm build_cjs esm_hack"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.136",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "shx": "^0.3.4"
  },
  "peerDependencies": {
    "@types/node": ">= 12",
    "@types/react": ">= 16",
    "react": ">= 16.14"
  },
  "peerDependenciesMeta": {
    "@types/node": {
      "optional": true
    },
    "@types/react": {
      "optional": true
    }
  }
}